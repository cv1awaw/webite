{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/verify.js",
            "use": "@vercel/node"
        },
        {
            "src": "api/admin.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/api/index.js"
        },
        {
            "src": "/api/verify",
            "dest": "/api/verify.js"
        },
        {
            "src": "/api/admin",
            "dest": "/api/admin.js"
        }
    ]
}